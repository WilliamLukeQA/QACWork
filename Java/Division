
public class Division {
	public static void main(String xyz[]){
		int a,b,c;
		a=b=c=0;
		
			try{
				a = Integer.parseInt(xyz[0]);
				b = Integer.parseInt(xyz[1]);
				c = a/b;
				System.out.println("The Results is " + c);
			}
			catch(NumberFormatException q){
				System.out.println("Please only numeric values");
			}
			catch(ArithmeticException w){
				System.out.println("You are not able to divide any value bt Zero");
			}
			catch(ArrayIndexOutOfBoundsException e){
				System.out.println("Please only enter two values");
			}
			catch(Exception x){
				System.out.println("What ya doing!?");
			}
	}
	
}
