
public class math3 {
	
	public int Add (int x, int y){
		int a;
		a = x+y;
		return a;
	}
	
	public int Multiply (int x, int y){
		int a;
		a = x*y;
		return a;
	}			

	public float Divide (int x, int y){
		int a;
		a = x/y;
		return a;
	}		

	public int Subtract (int x, int y){
		int a;
		a = x-y;
		return a;
	}		
	
}


