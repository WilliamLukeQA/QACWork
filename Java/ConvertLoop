
public class CovertLoop {
	public static void main (String x[]){
		String Binary = "101";
		int output = 0;
		for (int loop = 0; loop < Binary.length(); loop++){
			output = output * 2;
			
			if (Binary.charAt(loop) == '0'){
				output = output + 0;
			}
			else
				output = output + 1;
		}
		System.out.println(output);
	}
}
